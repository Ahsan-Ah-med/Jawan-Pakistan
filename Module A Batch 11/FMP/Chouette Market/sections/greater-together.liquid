  "blocks":[
  {
    "name": "Founders Image 1",
    "type": "image_1",
    "settings":[
        {
          "type": "image_picker",
          "id": "image_1",
          "label": "Image"
        },
        {
          "type": "color",
          "id": "image_bg",
          "label": "Paragraph Color",
          "default": "#1E2C21"
        },
       {
        "type": "text",
        "id": "avatar_name",
        "label": "Client Name",
        "default": "Bones"
       },
       {
        "type": "text",
        "id": "avatar_location",
        "label": "Client Location",
        "default": "Bones"
       },
      {
          "type": "image_picker",
          "id": "image_2",
          "label": "location_icon"
        },
        {
          "type": "range",
          "id": "image_position_left",
          "min": 1,
          "max": 100,
          "step": 1,
          "unit": "%",
          "label": "Circle Position Left",
          "default": 100
        },
       {
          "type": "range",
          "id": "image_position_top",
          "min": 1,
          "max": 100,
          "step": 1,
          "unit": "%",
          "label": "Circle Position Top",
          "default": 100

<div>
  <div class="founders-main">
    <div class="founders-main-container">
      <div class="founders-main-sub-container">
        <div class="founders-box-container-1">
          <div class="founders-text-container">
            <div class="founders-headings">{{ section.settings.heading_1 }}</div>
            <div class="founders-paragraph">{{ section.settings.paragraph_1 }}</div>
          </div>
          <div class="founders-image_1">
            {% for block in section.blocks %}
              {% case block.type %}
                {% when 'image_1' %}
                  <div class="image-container-box image-box-{{ forloop.index }}">
                    <img src="{{ block.settings.image_1 | img_url: 'master' }}">
                  </div>
              {% endcase %}
            {% endfor %}
          </div>
        </div>
        <div class="founders-box-container-2">
          <div class="founders-text-container">
            <div class="founders-headings">{{ section.settings.heading_2 }}</div>
            <div class="founders-paragraph">{{ section.settings.paragraph_2 }}</div>
          </div>
          <div class="founders-image_2" dir="rtl">
            {% for block in section.blocks %}
              {% case block.type %}
                {% when 'image_2' %}
                  <div class="image-container-box image-box-{{ forloop.index }}">
                    <img src="{{ block.settings.image_2 | img_url: 'master' }}">
                  </div>
              {% endcase %}
            {% endfor %}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<style>
       .founders-main{
         padding: {{  section.settings.top_padding }}px {{ section.settings.right_padding }}px {{ section.settings.bottom_padding }}px {{ section.settings.left_padding }}px;
         max-width: {{ section.settings.page_size }}px;
         width: 100%;
         margin: 0 auto;
       }
       .founders-main-container{
         padding: 20px;
         border: 1px solid {{ section.settings.border_body }};
         border-radius: 100px;
       }
       .founders-main-sub-container{
          background-color: {{ section.settings.background_color_body }};
           border-radius: 100px;
         padding: 60px 0px;
         box-shadow: 0px 4px 22px rgba(142.38, 138.82, 138.82, 0.22);
       }
       .founders-headings p{
         font-size: {{ section.settings.heading_font_size }}px;
         font-weight: {{ section.settings.heading_font_weight }};
         color: {{ section.settings.heading_color }};
         margin: 0;
         max-width: 410px;
         line-height: 52px;
         letter-spacing: 0px;
       }
       .founders-paragraph p{
         font-size: {{ section.settings.paragraph_font_size }}px;
         font-weight: {{ section.settings.paragraph_font_weight }};
         color: {{ section.settings.paragraph_color }};
         margin: 0;
         line-height: 24px;
         max-width: 340px;
           letter-spacing: 0px;
       }
       .founders-paragraph{
           text-align: start;
           width: 100%;
           max-width: 403px;
        }
       .founders-box-container-1{
          display: flex;
          align-items: center;
          justify-content: center;
          flex-wrap: wrap;
       }
      .founders-box-container-2{
          display: flex;
          align-items: center;
          justify-content: center;
          flex-wrap: wrap;
          flex-direction: row-reverse;
          padding-top: 60px;
       }
      .founders-box-container-2 .founders-text-container{
        width: 50%;
        padding: 0px 70px;
        display: flex;
        flex-wrap: wrap;
        justify-content: flex-start;
      }
            .founders-box-container-2 .founders-paragraph p {
      max-width: 365px;
  }
       .founders-image_1{
          display: flex;
          align-items: center;
          justify-content: center;
       }
  .founders-image_2{
              width: 50%;
              display: flex;
          align-items: center;
          justify-content: center;
              }
       .founders-text-container{
             width: 50%;
             padding: 0px 50px;
             display: flex;
             flex-wrap: wrap;
             justify-content: flex-end;
       }
       .founders-image_1{
             width: 50%;
       }

       .image-container-box{
          border: 1px solid #064E3B;
         border-radius: 100%;
         display: flex;
         flex-wrap: wrap;
         justify-content: center;
         align-items: center;
         padding: 10px;
         margin-right: 8px;
       }
       .founders-headings p strong{
          position: relative;
       }
       .founders-headings p strong:after{
          content: "";
          background-image:url('https://cdn.shopify.com/s/files/1/0789/9778/2834/files/cNkcef.png?v=1689602005');
          position: absolute;
          left: 0;
          bottom: -20px;
          max-width: 160px;
          width: 100%;
          height: 100%;
          background-position: center;
          background-repeat: no-repeat;
          background-size: contain;
       }
      {% for block in section.blocks %}
                   {% case block.type %}
                     {% when 'image_1' %}
       .image-box-{{ forloop.index }} img{
         background-color:{{ block.settings.image_bg }};
         border-radius: 100%;
         box-shadow: 0px 2.3470280170440674px 4.694056034088135px {{ block.settings.image_bg }};
       }
                      {% endcase %}
                 {% endfor %}
        .founders-box-container-2 .image-container-box img{
              box-shadow: 0px 2.3470280170440674px 4.694056034088135px #FFCF96;
              border-radius: 50%;
         }

        @media only screen and (max-width:1180px){
       .founders-text-container {
         padding: 0px 30px 0px 40px;
        }
        .founders-box-container-2 .founders-text-container {
            padding: 0px 40px 0px 30px;
        }
        .founders-headings p {
         font-size: {{ section.settings.heading_font_size_desktop }}px;
         max-width: 340px;
        }
       .founders-paragraph p {
         font-size: {{ section.settings.paragraph_font_size_desktop }}px;
         max-width: 300px;
       }
       .founders-paragraph {
         max-width: 300px;
      }
        }
        @media only screen and (max-width:900px){
       .founders-main{
         padding: {{ section.settings.top_padding_mobile }}px {{ section.settings.right_padding_mobile }}px {{ section.settings.bottom_padding_mobile }}px {{ section.settings.left_padding_mobile }}px;
       }
      .founders-box-container-2 .founders-text-container{
         width: 100%;
      }
      .founders-image_2{
        width: 100%;
      }
       .founders-text-container {
         padding: 0px 10px 15px 10px;
        }
        .founders-box-container-2 .founders-text-container {
            padding: 0px 10px 15px 10px;
        }
      .founders-paragraph {
          max-width: 300px;
      }
      .founders-main-container {
      padding: 10px;
      }
    .founders-main-sub-container {
      padding: 30px 0px;
    }
          .founders-text-container {
        width: 100%;
        justify-content: center;
        align-items: center;
        flex-direction: column;
    }
       .founders-image_1 {
        width: 100%;
    }
              .founders-box-container-2{
          padding-top: 30px;
       }
    .image-container-box{
                padding: 6px;
    }
                  }
     @media only screen and (max-width:767px){
    .founders-main-container {
      border-radius: 50px;
    }
    .founders-main-sub-container {
      border-radius: 50px;
    }
      .founders-headings p{
         font-size: {{ section.settings.fonderHeading }}px;
       }
       .founders-paragraph p {
            font-size: {{ section.settings.founderPara }}px;
        }
       .founders-paragraph p {
            max-width: 260px;
            margin: 0 auto;
        }
        .founders-headings p strong::after {
          bottom: -15px;
        }
        .founders-box-container-2 .founders-paragraph{
         max-width: 100%;
       }
        .founders-paragraph{
         max-width: 100%;
       }
       .founders-box-container-2 .founders-paragraph p {
          max-width: 260px;
      }
     }
</style>

<script type="text/javascript">
   $(document).ready(function() {
   $('.founders-image_1').slick({
     slidesToShow: 3,
    slidesToScroll: 1,
    autoplay: {{ section.settings.slick_autoplay }},
    autoplaySpeed: 0,
     dots: {{ section.settings.slick_dots }},
     arrows: {{ section.settings.slick_arrow }},
       speed: {{ section.settings.slick_autoplay_speed }},
         cssEase: 'linear',
         infinite: true,
      initialSlide: 1,
     centerPadding: '60px',
     centerMode: true,
     {% if section.settings.pre_arr != blank %}
     prevArrow:'<img src="{{ section.settings.pre_arr | img_url: 'master' }}"/>',
     {% endif %}
     {% if section.settings.next_arr != blank %}
     nextArrow:'<img src="{{ section.settings.next_arr | img_url: 'master' }}"/>',
     {% endif %}

     responsive: [
       {
        breakpoint: 1200,
        settings: {
          arrows: {{ section.settings.slick_arrow }},
          centerMode: false,
          centerPadding: '40px',
          slidesToShow: 3,
          slidesToScroll: 1,
        }
      },
      {
        breakpoint: 899,
        settings: {
          arrows: {{ section.settings.slick_arrow }},
          centerMode: false,
          centerPadding: '40px',
          slidesToShow: 5,
          slidesToScroll: 1,
        }
      },
      {
        breakpoint: 768,
        settings: {
          arrows: false,
          dots: false,
          centerMode: false,
          centerPadding: '40px',
          slidesToShow: 5,
          slidesToScroll: 1,
        }
      },
      {
        breakpoint: 600,
        settings: {
          arrows: false,
          centerMode: false,
          centerPadding: '40px',
          slidesToShow: 3,
          slidesToScroll: 1,
        }
      }
    ]
   });
   });
  $(document).ready(function() {
   $('.founders-image_2').slick({
     slidesToShow: 3,
    slidesToScroll: 1,
    autoplay: {{ section.settings.slick_autoplay }},
    autoplaySpeed: 0,
     rtl: true,
     dots: {{ section.settings.slick_dots }},
     arrows: {{ section.settings.slick_arrow }},
       speed: {{ section.settings.slick_autoplay_speed }},
         cssEase: 'linear',
         infinite: true,
      initialSlide: 1,
     centerPadding: '60px',
     centerMode: true,
     {% if section.settings.pre_arr != blank %}
     prevArrow:'<img src="{{ section.settings.pre_arr | img_url: 'master' }}"/>',
     {% endif %}
     {% if section.settings.next_arr != blank %}
     nextArrow:'<img src="{{ section.settings.next_arr | img_url: 'master' }}"/>',
     {% endif %}

     responsive: [
       {
        breakpoint: 1200,
        settings: {
          arrows: {{ section.settings.slick_arrow }},
          centerMode: false,
          centerPadding: '40px',
          slidesToShow: 3,
          slidesToScroll: 1,
        }
      },
      {
        breakpoint: 899,
        settings: {
          arrows: {{ section.settings.slick_arrow }},
          centerMode: false,
          centerPadding: '40px',
          slidesToShow: 5,
          slidesToScroll: 1,
        }
      },
      {
        breakpoint: 768,
        settings: {
          arrows: false,
          dots: false,
          centerMode: false,
          centerPadding: '40px',
          slidesToShow: 5,
          slidesToScroll: 1,
        }
      },
      {
        breakpoint: 600,
        settings: {
          arrows: false,
          centerMode: false,
          centerPadding: '40px',
          slidesToShow: 3,
          slidesToScroll: 1,
        }
      }
    ]
   });
   });
</script>
